{
  "hosting": {
    "target": "mythall",
    "public": "dist",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "**",
        "destination": "/error.html"
      }
    ]
  },
  "emulators": {
    "hosting": {
      "port": 3000
    }
  }
}
